        "drupal/metatag": "^2.0",
        "drupal/robotstxt": "^1.6",
        "drupal/seo_checklist": "5.2.x-dev",
        "drupal/simple_sitemap": "4.x-dev",
        "drupal/sitemap": "^2.0",
